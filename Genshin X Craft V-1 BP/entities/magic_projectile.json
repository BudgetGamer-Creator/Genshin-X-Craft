{
  "format_version": "1.20.50",
  "minecraft:entity": {
    "description": {
      "identifier": "gi:magic_projectile",
      "is_spawnable": false,
      "is_summonable": false
    },

    "components": {
      "minecraft:projectile": {
        "power": 1.6,
        "gravity": 0.02,
        "inertia": 0.99,
        "on_hit": {
          "impact_damage": {
            "damage": 12,
            "knockback": true
          },
          "remove_on_hit": {}
        }
      },

      "minecraft:collision_box": {
        "width": 0.25,
        "height": 0.25
      },

      "minecraft:timer": {
        "time": 60,
        "time_down_event": {
          "event": "despawn"
        }
      },

      "minecraft:particle_emitter": {
        "particle": "minecraft:enchanting_table_particle",
        "emit_rate": 8
      }
    },

    "events": {
      "despawn": {
        "remove": {}
      }
    }
  }
}
